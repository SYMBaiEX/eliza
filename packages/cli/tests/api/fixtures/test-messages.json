        }
      ],
      "expectedResponsePatterns": ["image", "see", "analyze"],
      "category": "image_processing"
    },
    {
      "text": "Process this file attachment",
      "attachments": [
        {
          "type": "text",
          "content": "Sample file content for testing",
          "filename": "test.txt",
          "contentType": "text/plain"
        }
      ],
      "expectedResponsePatterns": ["file", "attachment", "content"],
      "category": "file_processing"
    }
  ],

  "error_scenarios": [
    {
      "text": "",
      "expectedResponsePatterns": ["empty", "message", "error"],
      "category": "empty_message",
      "expectError": true
    },
    {
      "text": "A".repeat(10000),
      "expectedResponsePatterns": ["long", "message", "limit"],
      "category": "long_message",
      "expectError": false
    },
    {
      "text": "Special characters: !@#$%^&*(){}[]|\\:;\"'<>?,./",
      "expectedResponsePatterns": ["special", "characters"],
      "category": "special_characters"
    }
  ],

  "performance": [
    {
      "text": "Quick response test",
      "expectedResponseTime": 2000,
      "category": "response_time"
    },
    {
      "text": "Generate a detailed explanation of machine learning",
      "expectedResponseTime": 10000,
      "category": "complex_response"
    }
  ],

  "context": [
    {
      "sequence": [
        {
          "text": "Let's start a story about a robot",
          "expectedResponsePatterns": ["story", "robot", "start"]
        },
        {
          "text": "What happens next in our story?",
